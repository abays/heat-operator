{
  "command": "/usr/bin/heat-engine --config-dir /etc/heat/heat.conf.d",
  "config_files": [
    {
      "source": "/var/lib/config-data/merged/0*.conf",
      "dest": "/etc/heat/heat.conf.d/",
      "merge": false,
      "preserve_properties": true,
      "perm": "0644"
    }
  ],
  "permissions": [
    {
      "path": "/etc/heat/heat.conf.d",
      "owner": "heat:heat",
      "recurse": true
    }
  ]
}
